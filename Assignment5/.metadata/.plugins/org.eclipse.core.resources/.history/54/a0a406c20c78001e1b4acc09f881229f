package com.sunbeam;

public class BST 
{
	public class Node
	{
		private int data;
		private Node left;
		private Node right;
		
		Node(int data)
		{
			data = data;
			left = null;
			right = null;
		}
	}
	
	private Node root;
	
	BST()
	{
		
	}
	
	public boolean isEmpty()
	{
		return root == null);
	}
}
