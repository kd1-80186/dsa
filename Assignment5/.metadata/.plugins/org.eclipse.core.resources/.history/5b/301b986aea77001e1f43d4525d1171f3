package com.sunbeam;

public class Stack {
	List list;

	Stack() {
		list = new List();
	}

	public boolean isEmpty() {
		return list.isEmpty();
	}

	public void push(int value) {
		list.addFirst(value);
	}

	public void pop() {
		list.deleteFirst();
	}

	public void peek() {
		System.out.println("Topmost element : " + list.getValue());
	}

}
